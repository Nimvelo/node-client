{
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "comma-dangle": 0,
    "no-console": 0,
    "no-use-before-define": [2, "nofunc"],
    "padded-blocks": 0,
    "prefer-const": 1,
    "space-before-function-paren": 0,
    "strict": 0
  }
}
